<h1>Chat WebSocket</h1>
<div class="chat-container">
  <h5 id="h5"></h5>
  <form id="preventDefault-chat" onsubmit="return enviarMsg()">
    <input type="email" id="email-id" placeholder="Email Id" required="true" />
    <div class="main-user-data">
      <input type="name" id="nombre" placeholder="Nombre" required="true" />
      <input type="text" id="apellido" placeholder="Apellido" required="true" />
      <input type="number" id="edad" placeholder="Edad" required="true" />
    </div>
    <div class="main-user-data">
      <input type="text" id="alias" placeholder="Alias(Tu nombre en el chat)" />
      <input type="text" id="avatar" placeholder="Url Avatar" required="true" />
    </div>
    <p class="avatarUrl"></p>
    <div id="mgs-area" class="chat-area">
    </div>
    <div class="input-msg-container">
      <input type="text" id="inputMsg" placeholder="Mensaje" required="true" />
      <button class="my-button" type="submit">Enviar</button>
    </div>
  </form>
</div>